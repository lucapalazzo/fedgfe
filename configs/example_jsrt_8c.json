{
  "experiment": {
    "goal": "test",
    "device": "cuda",
    "device_id": "0",
    "runs": 1,
    "seed": 42
  },
  "federation": {
    "algorithm": "FedGFE",
    "num_clients": 8,
    "global_rounds": 50,
    "local_epochs": 5,
    "join_ratio": 1.0,
    "eval_gap": 1
  },
  "model": {
    "backbone": "vit",
    "backbone_model": "hf_vit",
    "pretrained": false,
    "num_classes": 2,
    "embedding_size": 768,
    "patch_size": 16
  },
  "training": {
    "optimizer": "SGD",
    "learning_rate": 0.01,
    "batch_size": 32,
    "weight_decay": 0.001,
    "momentum": 0.9,
    "learning_rate_schedule": true
