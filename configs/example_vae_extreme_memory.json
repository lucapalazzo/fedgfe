{
  "dataset": "ESC50",
  "algorithm": "A2V",
  "model": "ast",
  "batch_size": 2,
  "learning_rate": 0.0001,
  "num_rounds": 50,
  "num_clients": 5,
  "feda2v_config": {
    "generator_training_mode": true,
    "generator_granularity": "unified",
    "generator_type": "vae",
    "use_conditioned_vae": false,

    "generator_training_sequence_length": 4,
    "generator_output_sequence_length": null,

    "generator_training_epochs": 50,
    "generator_augmentation": true,
    "generator_augmentation_noise": 0.1,

    "diffusion_type": "sd",
    "classifier_pretrained_rounds": 5,
    "use_server_side_generation": false,
    "synthetic_samples_per_class": 5
  },
  "notes": [
    "⚠️ EXTREME memory constraints configuration (<4GB GPU)",
    "Uses only 4-token sequences (MAJOR information loss ~75-85%)",
    "Compression ratio: 303:1 (very aggressive)",
    "Requires only ~2-3 GB GPU VRAM",
    "⚠️ WARNING: Generated samples may lack fine details and hurt performance",
    "RECOMMENDED: Use sequence_length=32 instead if possible",
    "Consider this ONLY if you cannot use larger sequences"
  ]
}
